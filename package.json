{
  "name": "e3d-api",
  "version": "0.0.1",
  "description": "API to support the E3D Platform",
  "main": "bin/src/index.js",
  "typings": "src/**/*.ts",
  "keywords": [],
  "author": "Nicholas Mordecai",
  "scripts": {
    "prestart": "tsc",
    "start": "node bin/src/index.js",
    "dev": "nodemon --inspect src/index.ts",
    "setup-db": "ts-node test/setup.ts",
    "lint": "eslint --fix --ext .ts src/",
    "pretest": "tsc && tsnd test/setup.ts",
    "test": "ts-mocha --timeout 5000 --exit test/**/*.spec.ts",
    "coverage": "nyc ts-mocha --timeout 5000 --exit test/**/*.spec.ts",
    "posttest": "tsnd test/setup.ts"
  },
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.0.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@sentry/node": "^5.7.1",
    "@sentry/types": "^5.7.1",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mocha": "^5.2.6",
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "^12.7.5",
    "@types/request-promise": "^4.1.44",
    "@types/sinon": "^7.5.0",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^6.2.1",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "should": "^13.2.3",
    "sinon": "^7.5.0",
    "source-map-support": "^0.5.13",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.6.3"
  }
}
